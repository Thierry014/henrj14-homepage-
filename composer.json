{
  "require": {
    "craftcms/cms": "3.2.8",
    "craftcms/redactor": "2.3.3.2",
    "dolphiq/redirect": "1.0.23",
    "fatfish/redactor-grammar": "1.0.0",
    "mmikkel/retcon": "2.0.12",
    "nystudio107/craft-seomatic": "3.2.18",
    "solspace/craft-freeform": "3.2.3",
    "superbig/craft3-mobiledetect": "1.0.2",
    "verbb/field-manager": "2.1.0",
    "verbb/super-table": "2.2.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
